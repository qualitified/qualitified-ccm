<!-- See documentation at http://doc.nuxeo.com/x/HgIz -->
<package type="addon" name="nuxeo-liveconnect" version="1.3.3">
  <title>Nuxeo Live Connect</title>
  <description>
    <p>The Nuxeo Live Connect addon allows users to reference their files stored on most common EFSS systems (Dropbox, Box, Google Drive, One Drive). User can then apply business workflows to those external ressources, benefit from full-text search and edit metadata as if they were stored in the Nuxeo repository.</p>
  </description>
  <home-page>https://doc.nuxeo.com/nxdoc/nuxeo-live-connect/</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <!-- <hotreload-support>false</hotreload-support> -->
  <!-- <require-terms-and-conditions-acceptance>false</require-terms-and-conditions-acceptance> -->
  <!-- <nuxeo-validation>none</nuxeo-validation> -->
  <!-- <nuxeo-validation>inprocess</nuxeo-validation> -->
  <!-- <nuxeo-validation>primary_validation</nuxeo-validation> -->
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <!-- <production-state>proto</production-state> -->
  <!-- <production-state>testing</production-state> -->
  <production-state>production_ready</production-state>
  <supported>true</supported>
  <platforms>
    <platform>server-10.10</platform>
    <platform>server-10.10-*</platform>
  </platforms>
  <optional-dependencies>
    <package>nuxeo-jsf-ui</package>
    <package>nuxeo-web-ui</package>
  </optional-dependencies>
  <license>LGPL 2.1</license>
  <license-url>http://www.gnu.org/licenses/lgpl-2.1.html</license-url>
  <visibility>PUBLIC</visibility>
</package>
